<Window x:Class="lab_2_mav.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab_2_mav"
        mc:Ignorable="d" ResizeMode="NoResize"
        Title="MainWindow" Height="450" Width="800">

    <Window.DataContext>
        <local:InformatiiList />
    </Window.DataContext>

    <Grid>

        <Button x:Name="Administrativ" Content="Administrativ" HorizontalAlignment="Center" Margin="0,53,0,0" VerticalAlignment="Top" Width="160" Click="button_Click" Height="59"/>
        <Button x:Name="Cautare_de_cuvinte" Content="Cautare de cuvinte" HorizontalAlignment="Center" Margin="0,183,0,0" VerticalAlignment="Top" Width="160" Click="button_Click_1" Height="59"/>
        <Button x:Name="Divertisment" Content="Divertisment" HorizontalAlignment="Center" Margin="0,313,0,0" VerticalAlignment="Top" Width="160" Click="button_Click_2" Height="59"/>
    </Grid>
</Window>
