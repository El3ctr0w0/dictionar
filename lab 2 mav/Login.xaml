<Window x:Class="lab_2_mav.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab_2_mav"
        mc:Ignorable="d" ResizeMode="NoResize"
        Title="Login" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="Username" Text="Enter your username here!" Width="290" Height="50" Margin="255,114,255,270"></TextBox>
        <TextBox x:Name="Password" Text="Enter your password here!" Width="290" Height="50" Margin="255,251,255,133"></TextBox>
        <Button x:Name="LogIN" Content="Login" Margin="375,329,375,55" Click="LogIN_Click"></Button>
        <TextBlock x:Name="TextB1" Text="Insert your username and password bellow to login" Margin="35,32,33,325" FontSize="30"/>
    </Grid>
</Window>
